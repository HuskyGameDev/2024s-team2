.button {
    height: 38px;
    -unity-font-style: normal;
    border-top-left-radius: 9px;
    border-top-right-radius: 9px;
    border-bottom-right-radius: 9px;
    border-bottom-left-radius: 9px;
}

.text {
    font-size: 24px;
    -unity-font-style: bold;
    letter-spacing: 5px;
    -unity-text-outline-width: 0.5px;
    margin-top: 0;
    margin-bottom: 0;
    padding-top: 8px;
}
